    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "husky": "^4.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "1.19.1",
    "snowpack": "^1.6.0",
    "typescript": "^3.8.3",
    "typescript-eslint-language-service": "^2.1.8"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm test",
      "pre-push": "npm test"
    }
  }
